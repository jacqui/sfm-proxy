{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "pattern": "^[a-z]{2}$"
    },
    "name": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "events_count": {
      "type": "integer"
    }
  },
  "links": [{
    "rel": "alternate",
    "href": "{id}.zip",
    "mediaType": "application/zip"
  }, {
    "rel": "alternate",
    "href": "{id}.txt",
    "mediaType": "text/plain"
  }]
}
